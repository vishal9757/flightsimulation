<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.avalia.learning.java.flightsimulation.Flight" table="flight">
		<id name="FlightNum">
			<generator class="assigned"></generator>
		</id>
		<property name="ArrLoc"></property>
		<property name="DepLoc"></property>
		<property name="Fare"></property>
		<property name="FlightDuration"></property>
		<property name="FlightTime"></property>
		<property name="ValidTill"></property>

	</class>

</hibernate-mapping>  